{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@scraping/*":["src/modules/scraping/*"],
      "@parser/*":["src/classes/scraping/*"],
      "@file/crawling/*":["src/storage/file/crawling/*"],
      "@file/history/*":["src/storage/file/history/*"],
    }
  }
}
